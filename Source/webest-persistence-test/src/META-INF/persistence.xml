			<property name="openjpa.ConnectionUserName" value="webest" />
			<property name="openjpa.ConnectionPassword" value="webest" />
			<property name="openjpa.InverseManager" value="true"/>
			<property name="openjpa.Log" value="SQL=TRACE" />
